--test--
Example test functions

--file--

$n = 'parsa'
func myfunc
    println $n
    $n = 'myfunc'
    println $n;
endfunc

func tststs
    println $n
    $n = 'tststs'
    myfunc()
    println $n;
endfunc

println $n
tststs
println $n

--output--
"""parsa
parsa
tststs
myfunc
tststs
parsa
"""
