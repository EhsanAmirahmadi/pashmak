--test--
inline function calling name conflicts bug is fixed

--file--
func hello()
        println 'hello'
endfunc

func hel
        println 'hel'
endfunc

mem hello(), hel()
mem hel(), hello()
mem hello(hel())
mem hel(hello())

$variable = 'variable'
$variant = 'variant'
$vari = 'vari'
println($variable + ' ' + $variant)
println($variant + ' ' + $variable)
println($variant + $vari + ' ' + $variable)

--output--
"""hello
hel
hel
hello
hel
hello
hello
hel
variable variant
variant variable
variantvari variable
"""
