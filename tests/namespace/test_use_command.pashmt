--test--
Test use command in namespaces

--file--
namespace App;
    func dosomething;
        print 'hello world\n';
    endfunc;
    set $name; mem 'the app'; copy $name;
endns;

namespace Second;
    func hello;
        print 'hello\n';
    endfunc;
    set $test; mem 'the second'; copy $test;
endns;

App.dosomething;
Second.hello;
out $App.name;
out $Second.test;

use App;
use Second;

App.dosomething;
dosomething;
Second.hello;
hello;

out $App.name;
out $Second.test;
out $name;
out $test;

--output--
'hello world\nhello\nthe appthe secondhello world\nhello world\nhello\nhello\nthe appthe secondthe appthe second'
