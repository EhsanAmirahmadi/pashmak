
func hi
    println('start')

    func bye
        println('bye')
    endfunc

    func foo
        println('foo start')
        func bar
            println('bar')
        endfunc
        bar()
        println('foo end')
    endfunc

    bye()

    foo()

    println('end')
endfunc

hi()
bye()

