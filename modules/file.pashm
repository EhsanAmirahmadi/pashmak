
alias file.open;
    set $args; copy $args;
    mem open($args[0] , $args[1]);
    free $args;
endalias;

alias file.close;
    set $file; copy $file;
    mem $file.close();
    free $file;
endalias;

alias file.read;
    set $file; copy $file;
    mem $file.read();
    free $file;
endalias;

alias file.write;
    set $args; copy $args;
    mem $args[0].write($args[1]);
    free $args;
endalias;
