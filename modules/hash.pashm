
namespace hash;

	func blake2b ($value);
		py 'self.mem = hashlib.blake2b("' + $value + '".encode()).hexdigest()';
	endfunc;

	func blake2s ($value);
		py 'self.mem = hashlib.blake2s("' + $value + '".encode()).hexdigest()';
	endfunc;

	func md5 ($value);
		py 'self.mem = hashlib.md5("' + $value + '".encode()).hexdigest()';
	endfunc;

	func sha1 ($value);
		py 'self.mem = hashlib.sha1("' + $value + '".encode()).hexdigest()';
	endfunc;

