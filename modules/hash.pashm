
namespace hash;

	func sha256;
		set $value; copy $value;
		py 'self.mem = hashlib.sha256("' + $value + '".encode()).hexdigest()';
		free $value;
	endfunc;

	func md5;
		set $value; copy $value;
		py 'self.mem = hashlib.md5("' + $value + '".encode()).hexdigest()';
		free $value;
	endfunc;

endnamespace;
