alias random.randint;
    set $random.randint.temp;
    copy $random.randint.temp;
    mem 'self.mem = random.randint(' + str($random.randint.temp[0]) + ',' + str($random.randint.temp[1]) + ')';
    python ^;
endalias;

alias random.random;
    mem 'self.mem = random.random()';
    python ^;
endalias;
